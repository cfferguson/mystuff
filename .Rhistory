distribution <-
read_csv("qualtrics_distribution.csv") %>%
rename(Name = `First Name`,
email = Email) %>%
mutate(Name = str_to_title(Name),
email = str_to_lower(email)) %>%
select(Name, email, Reason) %>%
left_join(select(members, `ORCiD iD`, Name), by = "Name"); distribution
# Pull from ORCiD API ----
