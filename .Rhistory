distribution %>%
separate(Name, c("First", "Last")) %>%
mutate_if(is.character, str_squish) %>%
select(-c(`ORCiD iD`, Reason, email)) %>%
mutate(tag = "West Virginia") %>%
add_column("ORCiD" = NA); morcid
